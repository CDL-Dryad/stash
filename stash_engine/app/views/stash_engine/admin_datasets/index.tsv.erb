<%= "Title\tStatus\tAuthor\tDOI\tLast modified\tLast modified by\tSize\tPublication date" %>
<% @ds_identifiers.each do |i| -%>
<%
    latest_resource = i&.latest_resource
    row = [ latest_resource&.title || '[no title set]',
            latest_resource&.latest_curation_status.status,
            latest_resource&.user&.name,
            i.identifier,
            formatted_datetime(latest_resource&.latest_curation_status&.updated_at),
            latest_resource&.latest_curation_status&.user&.name,
            i.storage_size,
            "My last modified"
    ]
-%><%= row.join("\t") %>
<% end -%>